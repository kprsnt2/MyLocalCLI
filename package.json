  "description": "Your Own AI Coding Assistant - Private, Local, Yours. Claude Code alternative with 22 skills, 26 tools, and 5 agents.",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "mylocalcli": "src/index.js",
    "mlc": "src/index.js"
  },
  "files": [
    "src",
    "docs"
  ],
  "scripts": {
    "start": "node src/index.js",
    "dev": "node src/index.js",
    "web": "node src/index.js web",
    "test": "vitest run",
