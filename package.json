  "author": "kprsnt2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kprsnt2/MyLocalCLI.git"
  },
  "homepage": "https://github.com/kprsnt2/MyLocalCLI",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^12.0.0",
    "figures": "^6.0.1",
    "glob": "^10.3.10",
    "inquirer": "^13.1.0",
    "marked": "^11.1.1",
    "marked-terminal": "^6.2.0",
    "ora": "^8.0.1"
  },
